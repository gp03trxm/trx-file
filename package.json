{
  "name": "trx-file",
  "version": "1.0.35",
  "description": "trx file",
  "export": "index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "echo pass",
    "start": "npm run build && node -r source-map-support/register -r dotenv/config dist/index.js",
    "start:dev": "npm run build:watch & nodemon -r source-map-support/register -r dotenv/config --watch dist dist/index.js",
    "start:pm2": "npm run build && pm2 startOrReload ecosystem.config.cjs --env production --update-env",
    "start:pm2:docker": "npm run build && pm2 startOrReload ecosystem.config.cjs --no-daemon --env production --update-env",
    "upload:smshook": "./scripts/upgrade-smshook.sh",
    "bump": "npm version patch && git push --tags"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^5.8.0",
    "@pm2/io": "^4.3.3",
    "@trx/trx-captcha": "^1.0.8",
    "@trx/trx-log": "^1.0.11",
    "@trx/trx-types": "^2.0.52",
    "@types/node-fetch": "^2.5.5",
    "@types/sharp": "^0.24.0",
    "anticaptcha": "^2.0.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "formidable": "^1.2.2",
    "lodash": "^4.17.21",
    "lowdb": "^2.1.0",
    "multer": "^1.4.3",
    "node-fetch": "^2.6.0",
    "pm2": "^5.1.1",
    "qrcode": "^1.4.4",
    "serialize-error": "^8.0.1",
    "sharp": "^0.29.0",
    "uuid": "^7.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.2",
    "@types/formidable": "^1.2.3",
    "@types/lowdb": "^1.0.9",
    "@types/multer": "^1.4.2",
    "@types/uuid": "^7.0.0",
    "nodemon": "^2.0.2",
    "prettier": "^2.2.1",
    "tslint": "^6.0.0",
    "typescript": "^4.3.5"
  },
  "type": "module",
  "engines": {
    "node": "v14.17.5",
    "npm": "7.21.0"
  }
}
